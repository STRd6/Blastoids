Level = 
  load: (data) ->
    {walls, spawnPoints} = data

    walls.each ([start, end]) ->
      engine.add
        class: "Wall"
        start: Point(start.x, start.y)
        end: Point(end.x, end.y)

    window.spawnPoints = spawnPoints.map ({x, y}) ->
      Point(x, y)

